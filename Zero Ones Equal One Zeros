#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define op() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
const int mx=5e3+123;
int a[mx];
int main()
{
  op();
  int t;
  cin>>t;
  while(t--)
  {
    int n;
    cin>>n;
    if(n%2==0)
    {
      cout<<"1";
      for(int i=1; i<=n-2; i++)
      {
        cout<<"0";
      }
      cout<<"1";
    }
    else
    {
      cout<<"0";
      for(int i=1; i<=n-2; i++)
      {
        cout<<"1";
      }
      cout<<"0";
    }
    cout<<endl;
  }

}
